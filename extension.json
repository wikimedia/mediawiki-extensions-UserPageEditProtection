{
	"name": "UserPageEditProtection",
	"version": "4.2.0",
	"author": [
		"Lisa Ridley",
		"Eric Gingell",
		"Karsten Hoffmeyer",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:UserPageEditProtection",
	"descriptionmsg": "userpageeditprotection-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.36.0"
	},
	"AvailableRights": [
		"editalluserpages"
	],
	"GroupPermissions": {
		"sysop": {
			"editalluserpages": true
		}
	},
	"MessagesDirs": {
		"UserPageEditProtection": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\UserPageEditProtection\\": "includes/"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\UserPageEditProtection\\UserPageEditProtection"
		}
	},
	"Hooks": {
		"getUserPermissionsErrors": "main"
	},
	"config": {
		"OnlyUserEditUserPage": {
			"value": false
		}
	},
	"manifest_version": 2
}
